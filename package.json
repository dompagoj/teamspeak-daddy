{
  "name": "ljopi-discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "tsc && NODE_ENV=dev node build",
    "dev": "tsnd --respawn src",
    "deploy": "npm run build && pm2 restart ljopi_discord_bot",
    "clean": "rm -rf build && rm tsconfig.tsbuildinfo",
    "build": "tsc",
    "migrate:create": "knex migrate:make $1 --env all -x ts",
    "migrate:latest": "knex migrate:latest --env all",
    "migrate:rollback": "knex migrate:rollback --env all",
    "db:init": "docker-compose up -d"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "knex": "^2.4.2",
    "mathjs": "^11.7.0",
    "pg": "^8.10.0",
    "ts3-nodejs-library": "^3.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/knex": "^0.16.1",
    "@types/mathjs": "^9.4.2",
    "@types/node": "18.15.7",
    "@types/pg": "^8.6.6",
    "nodemon": "^2.0.22",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  }
}
